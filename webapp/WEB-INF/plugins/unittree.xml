<?xml version="1.0" encoding="UTF-8" standalone="no"?><plug-in>
    <!-- Plugin Informations -->
    <name>unittree</name>
    <class>fr.paris.lutece.plugins.unittree.service.UnitTreePlugin
    </class>
    <version>3.0.0</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>unittree.plugin.description</description>
    <provider>unittree.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/unittree/unittree.png</icon-url>
    <copyright>Copyright 2001-2011 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>3.1.1</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- if the plugin must have a connection pool with parameter : 1 - yes, 
        0 - no -->
    <db-pool-required>1</db-pool-required>

    <!--Administration interface parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>UNITS_MANAGEMENT</feature-id>
            <feature-title>unittree.adminFeature.unitsManagement.name
            </feature-title>
            <feature-description>unittree.adminFeature.unitsManagement.description
            </feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/unittree/ManageUnits.jsp
            </feature-url>
            <feature-icon-url>images/admin/skin/plugins/unittree/unittree.png
            </feature-icon-url>
            <feature-documentation-url/>
        </admin-feature>
    </admin-features>

    <!-- RBAC Resources -->
    <rbac-resource-types>
        <rbac-resource-type>
            <rbac-resource-type-class>
                fr.paris.lutece.plugins.unittree.service.unit.UnitResourceIdService
            </rbac-resource-type-class>
        </rbac-resource-type>
    </rbac-resource-types>

    <!-- Freemarker -->
    <freemarker-macro-files>
        <freemarker-macro-file>/admin/plugins/unittree/unittree_commons.html
        </freemarker-macro-file>
    </freemarker-macro-files>
</plug-in>